<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="stock_picking_pob_access_point_id" model="ir.ui.view">
        <field name="name">stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <field name="location_dest_id" position="after">
                <label for="presta_ups_access_point_id" />
                <div class="o_row">
                    <field name="presta_ups_access_point_id" />
                    <span>(<a
              href="https://www.ups.com/dropoff?loc=en_nl"
              target="new"
            >UPS Access Points</a>)</span>
                </div>
                <label for="presta_ups_access_point_country" />
                <div class="o_row">
                    <field name="presta_ups_access_point_country" />
                    <span>(<a
              href="https://www.ups.com/content/us/en/codes.html"
              target="new"
            >UPS Access Point Countries</a>)</span>
                </div>
            </field>
        </field>
    </record>

</odoo>
