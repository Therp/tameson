#. module: tameson_mail
#: model:mail.template,subject:tameson_mail.email_template_edi_invoice_tameson
msgid "${object.company_id.name} Invoice (Ref ${object.name or 'n/a'})"
msgstr "Factura de ${object.company_id.name} (Ref ${object.name or 'n/a'})"

#. module: tameson_mail
#: model:mail.template,body_html:tameson_mail.email_template_edi_invoice_tameson
msgid ""
"<div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"Dear \n"
"% if object.partner_id.parent_id:\n"
"    ${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"% else:\n"
"    ${object.partner_id.name},\n"
"% endif\n"
"<br/><br/>\n"
"    <p>Here is your new invoice: </p>\n"
"    <br/>\n"
"    <p style=\"border-left: 1px solid #8e0000; margin-left: 30px; padding-left: 5px;\">\n"
"        <strong>REFERENCES</strong><br/>\n"
"        Invoice Number : <strong>${object.name}</strong><br/>\n"
"        Total Amount: <strong>${object.amount_total} ${object.currency_id.name}</strong><br/>\n"
"        Invoice Date: ${object.invoice_date}<br/>\n"
"        % if object.origin:\n"
"        Origin: ${object.origin}<br/>\n"
"        % endif\n"
"        <br/>\n"
"        % set ref = object.get_so_ref()\n"
"        % if ref:\n"
"            <span>Order message: ${ref} </span>\n"
"            <br/>\n"
"        % endif\n"
"        % if object.invoice_payment_state != 'paid' and object.payment_term_id:\n"
"            <span>Payment term: ${object.payment_term_id.name}</span>\n"
"            <br/>\n"
"        % endif\n"
"        % if object.invoice_payment_state == 'paid':\n"
"            <strong>Notice: This invoice has been paid.</strong>\n"
"            <br/>\n"
"        % endif\n"
"    </p>\n"
"    <br/>\n"
"    <p>Do not hesitate to contact us if you have any questions.</p>\n"
"    <p>Thank You for choosing ${object.company_id.name or 'us'}</p>\n"
"    <br/>\n"
"    <br/>\n"
"</div>\n"
"            "
msgstr "<div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"Estimado/a\n"
"% if object.partner_id.parent_id: \n"
"${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"% else: \n"
"${object.partner_id.name},\n"
"% endif \n"
"<br/> <br /> \n"
"<p> Aquí está su nueva factura: </p> \n"
"<br/> \n"
"<p style = \"border-left: 1px solid # 8e0000; margin-left: 30px; padding-left: 5px;\"> \n"
"<strong> REFERENCIAS </strong> <br/> \n"
"Número de factura: <strong> ${object.name} </strong><br/> \n"
"Importe total: <strong> ${object.amount_total} ${object.currency_id.name} </strong> <br/> \n"
"Fecha de la factura: ${object.invoice_date} <br/>\n"
"% if object.origin: \n"
"Origin: ${object.origin}<br/>\n"
"% endif \n"
"<br/>\n"
"% set ref = object. get_so_ref ()\n"
"% if ref: \n"
"<span> Mensaje de pedido: ${ref} </span> \n"
"<br/>\n"
"% endif\n"
"% if object.invoice_payment_state != 'paid' and object.payment_term_id: \n"
"<span> Plazo de pago: ${ object.payment_term_id.name} </span> \n"
"<br/>\n"
"% endif\n"
"\n"
"% if object.invoice_payment_state == 'pagado': \n"
"<strong> Aviso : Esta factura ha sido pagada.</strong> \n"
"<br/>\n"
"% endif \n"
"</p> \n"
"<br/> \n"
"<p> No dude en contactarnos si tiene alguna pregunta. </p> \n"
"<p> Gracias por eligiendo ${object.company_id.name or 'us'}</p> <br/> <br/> </div>\n"
"            "

