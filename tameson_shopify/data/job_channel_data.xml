        <field name="name">shopify_stock_sync</field>
        <field name="parent_id" ref="queue_job.channel_root" />
    </record>
    <record id="function_channel_stock_sync" model="queue.job.function">
        <field name="model_id" ref="shopify_ept.model_shopify_process_import_export" />
