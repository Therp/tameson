        <field name="channel_id" ref="channel_stock_sync" />
    </record>
    <record id="function_sync_mismatch_qty" model="queue.job.function">
        <field name="model_id" ref="shopify_ept.model_shopify_process_import_export" />
        <field name="method">sync_mismatch_qty</field>
        <field name="channel_id" ref="channel_stock_sync" />
    </record>
    <record id="function_create_missing_maps" model="queue.job.function">
        <field name="model_id" ref="shopify_ept.model_shopify_process_import_export" />
        <field name="method">create_missing_maps</field>
        <field name="channel_id" ref="channel_stock_sync" />
    </record>
</odoo>
